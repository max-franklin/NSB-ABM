library(ggplot2)
library(ggplot2)
library(mvtnorm)
library(KernSmooth)
data <- read.csv(file.choose(), header = F)
data
p <- ggplot(data, aes(V1,V2))
p <- p + stat_bin2d(bins = 20)
p
z <- bkde2D(data, 3)
persp(z$fhat, theta = 120, phi = 20, xlab = "X", ylab = "Y", zlab = "f(x,z)")
mat <- z$fhat
foo <- apply(t(mat),2,rev)
write.delim(foo, file = " ", row.names = FALSE, col.names = FALSE)
write.csv(foo, file = " ", row.names = FALSE, col.names = FALSE)
ggplot(data, aes(V1,V2))+geom_density2d()+labs(x = "X", y = "Y")
